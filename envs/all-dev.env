ANTINEX_EXCHANGE_NAME=webapp.predict.requests
ANTINEX_EXCHANGE_TYPE=topic
ANTINEX_QUEUE_NAME=webapp.predict.requests
ANTINEX_WORKER_SSL_ENABLED=0
ANTINEX_CA_CERTS=
