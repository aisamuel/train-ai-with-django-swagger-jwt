ENVIRONMENT=Development
DJANGO_CONFIGURATION=Development
DJANGO_SECRET_KEY=supersecret
DJANGO_DEBUG=yes
DJANGO_TEMPLATE_DEBUG=yes
WEBAPP_HOST=0.0.0.0
WEBAPP_PORT=8080
COLLECT_STATICS=1
